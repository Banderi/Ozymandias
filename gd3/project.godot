[application]

config/name="Ozymandias"
run/main_scene="res://scenes/Root.tscn"
run/flush_stdout_on_print=true
config/icon="res://icon.png"
run/main_loop_type="CustomMainLoop"

[autoload]

Sounds="*res://scripts/autoload/Sounds.gd"
Log="*res://scripts/autoload/Log.gd"
IO="*res://scripts/autoload/IO.gd"
Debug="*res://scripts/autoload/Debug.gd"
Keybinds="*res://scripts/autoload/Keybinds.gd"
Settings="*res://scripts/autoload/Settings.gd"
Game="*res://scripts/autoload/game/Game.gd"
Assets="*res://scripts/autoload/Assets.gd"
Family="*res://scripts/autoload/game/Family.gd"
Stopwatch="*res://scripts/autoload/Stopwatch.gd"
Scribe="*res://scripts/autoload/Scribe.gd"
Campaign="*res://scripts/autoload/game/Campaign.gd"
Map="*res://scripts/autoload/game/Map.gd"
PKWareMono="*res://scripts/mono/PKWare_mono.cs"
